  "tokenColors": [
    {
      "name": "Comment",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#8FA39B",
        "fontStyle": "italic"
      }
    },
    {
      "name": "String",
      "scope": [
        "string",
        "string.quoted",
        "string.template"
      ],
      "settings": {
        "foreground": "#3FBF9A"
      }
    },
    {
      "name": "Number",
      "scope": [
        "constant.numeric"
      ],
      "settings": {
        "foreground": "#C2A86B"
      }
    },
    {
      "name": "Built-in constant",
      "scope": [
        "constant.language",
        "constant.character",
        "constant.other"
      ],
      "settings": {
        "foreground": "#C2A86B"
      }
    },
    {
      "name": "User-defined constant",
      "scope": [
        "variable.other.constant"
      ],
      "settings": {
        "foreground": "#C2A86B"
      }
    },
    {
      "name": "Keyword",
      "scope": [
        "keyword",
        "keyword.control",
        "keyword.operator.new",
        "keyword.other"
      ],
      "settings": {
        "foreground": "#C2A86B"
      }
    },
    {
      "name": "Storage",
      "scope": [
        "storage",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#C2A86B"
      }
    },
    {
      "name": "Operators",
      "scope": [
        "keyword.operator"
      ],
      "settings": {
        "foreground": "#4FA6A3"
      }
    },
    {
      "name": "Function name",
      "scope": [
        "entity.name.function",
        "meta.function-call",
        "support.function"
      ],
      "settings": {
        "foreground": "#4FA6A3"
      }
    },
    {
      "name": "Class name",
      "scope": [
        "entity.name.class",
        "entity.name.type.class",
        "support.class"
      ],
      "settings": {
        "foreground": "#6E8FA6"
      }
    },
    {
      "name": "Type",
      "scope": [
        "entity.name.type",
        "support.type",
        "entity.other.inherited-class"
      ],
      "settings": {
        "foreground": "#6E8FA6"
      }
    },
    {
      "name": "Tag name",
      "scope": [
        "entity.name.tag",
        "meta.tag.sgml"
      ],
      "settings": {
        "foreground": "#C2A86B"
      }
    },
    {
      "name": "Tag attribute",
      "scope": [
        "entity.other.attribute-name"
      ],
      "settings": {
        "foreground": "#4FA6A3"
      }
    },
    {
      "name": "Variable",
      "scope": [
        "variable",
        "variable.other"
      ],
      "settings": {
        "foreground": "#C9D6CF"
      }
    },
    {
      "name": "Variable parameter",
      "scope": [
        "variable.parameter"
      ],
      "settings": {
        "foreground": "#6E8FA6"
      }
    },
    {
      "name": "Import/Export",
      "scope": [
        "keyword.control.import",
        "keyword.control.export",
        "keyword.control.from"
      ],
      "settings": {
        "foreground": "#C2A86B"
      }
    },
    {
      "name": "JSON Key",
      "scope": [
        "support.type.property-name.json",
        "meta.structure.dictionary.key.json"
      ],
      "settings": {
        "foreground": "#4FA6A3"
      }
    },
    {
      "name": "Markup Bold",
      "scope": [
        "markup.bold"
      ],
      "settings": {
        "foreground": "#C2A86B",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markup Italic",
      "scope": [
        "markup.italic"
      ],
      "settings": {
        "foreground": "#C2A86B",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markup Heading",
      "scope": [
        "markup.heading",
        "entity.name.section"
      ],
      "settings": {
        "foreground": "#6E8FA6",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markup Link",
      "scope": [
        "markup.underline.link"
      ],
      "settings": {
        "foreground": "#3FBF9A"
      }
    },
    {
      "name": "Markup Code",
      "scope": [
        "markup.inline.raw",
        "markup.fenced_code.block"
      ],
      "settings": {
        "foreground": "#4FA6A3"
      }
    },
    {
      "name": "Markup Quote",
      "scope": [
        "markup.quote"
      ],
      "settings": {
        "foreground": "#8FA39B",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Invalid",
      "scope": [
        "invalid",
        "invalid.illegal"
      ],
      "settings": {
        "foreground": "#C06C5F"
      }
    },
    {
      "name": "Deprecated",
      "scope": [
        "invalid.deprecated"
      ],
      "settings": {
        "foreground": "#C06C5F",
        "fontStyle": "strikethrough"
      }
    },
    {
      "name": "Punctuation",
      "scope": [
        "punctuation.definition",
        "punctuation.separator",
        "punctuation.terminator"
      ],
      "settings": {
        "foreground": "#8FA39B"
      }
    },
    {
      "name": "Meta Brace",
      "scope": [
        "meta.brace",
        "punctuation.section"
      ],
      "settings": {
        "foreground": "#C9D6CF"
      }
    }
  ]
}